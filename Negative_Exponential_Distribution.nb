 RowBox[{
  RowBox[{"Psi", "[", "a_", "]"}], " ", ":=", " ", 
  RowBox[{
   RowBox[{"1", "/", 
    RowBox[{"(", 
     RowBox[{"1", "-", "a"}], ")"}]}], "*", 
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{"x", "*", 
      RowBox[{"f", "[", "x", "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", 
       RowBox[{"G", "[", "a", "]"}], ",", " ", "0"}], "}"}]}], 
    "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Psi", "[", "a_", "]"}], " ", ":=", " ", 
  FractionBox[
   RowBox[{
    RowBox[{"-", "1"}], "+", "a", "-", 
    RowBox[{"a", " ", 
